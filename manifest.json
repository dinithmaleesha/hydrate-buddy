{
    "manifest_version": 3,
    "name": "Hydrate Buddy",
    "version": "1.0",
    "description": "A Chrome extension to remind you to drink water.",
    "permissions": ["notifications", "alarms", "storage"],
    "background": {
      "service_worker": "background.js"
    },
    "icons": {
      "16": "icons/icon-16.png",
      "48": "icons/icon-48.png",
      "128": "icons/icon-128.png"
    }
}